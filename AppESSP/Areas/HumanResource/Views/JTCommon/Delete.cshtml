@model ESSPCORE.EF.JTCommon


<div class="modal-header bg-blue">
    <button type="button" class="close" data-dismiss="modal">&times;</button>
    <h4 class="modal-title">
        <i class="icon-menu7"></i> &nbsp;Common JobTitle
    </h4>
</div>
<div class="modal-body">
    <div class="form-horizontal">
        <div class="form-group text-center h5 text-black text-blue-800">
            <span>Are you sure you want to delete this?</span>
        </div>
        @using (Html.BeginForm(null, null, FormMethod.Get, new { name = "formDeleteID", id = "formDeleteID" }))
        {
            @Html.AntiForgeryToken()
            @Html.HiddenFor(model => model.PJTCommonID)
        }
    </div>
</div>
<div class="modal-footer">

    @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary bg-primary-300" })
    <input type="button" id="btnPostDelete" class="btn btn-danger bg-danger-800" value="Delete" />

</div>

<script type="text/javascript">
        $(document).ready(function () {
            JTCommonPostDelete();
        });
</script>

