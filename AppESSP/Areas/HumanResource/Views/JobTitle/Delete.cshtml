@model ESSPCORE.EF.JobTitle


@Html.HiddenFor(model => model.PJobTitleID)
@Html.HiddenFor(model => model.JobTitleName)

<div class="modal-header bg-blue">
    <button type="button" class="close" data-dismiss="modal">&times;</button>
    <h4 class="modal-title">
        <i class="icon-menu7"></i> &nbsp;
    </h4>
</div>
<div class="modal-body">
    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-md-3 text-semibold">
                @Html.DisplayNameFor(model => model.JobTitleName)
            </div>
            <div class="col-md-9">
                @Html.DisplayFor(model => model.JobTitleName)
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-3 text-semibold">
                @Html.DisplayNameFor(model => model.JTCommonID)
            </div>
            <div class="col-md-9">
                @Html.DisplayFor(model => model.JTCommonID)
            </div>
        </div>
        
        @using (Html.BeginForm(null, null, FormMethod.Get, new { name = "formDeleteID", id = "formDeleteID" }))
        {
            @Html.AntiForgeryToken()
            @Html.HiddenFor(model => model.PJobTitleID)
            @Html.HiddenFor(model => model.JobTitleName)
        }
    </div>
</div>
<div class="modal-footer">
    @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary bg-primary-300" })
    <input type="button" id="btnPostDelete" class="btn btn-danger bg-danger-800" value="Delete" />
</div>
<script type="text/javascript">
    $(document).ready(function () {
        JobTitlePostDelete();
    });
</script>
